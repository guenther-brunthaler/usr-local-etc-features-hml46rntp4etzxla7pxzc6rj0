	address 10.246.55.555
	netmask 255.255.0.0
	wireless-essid example-unencrypted-hotspot
	wireless-mode accesspoint
	#gateway 10.246.555.555
	# dns-* options are implemented by the resolvconf package, if installed
	dns-nameservers 9.9.9.9
	dns-search wifi.wlan.emgenxx69lwyn5ctlr4nl64ul.local
