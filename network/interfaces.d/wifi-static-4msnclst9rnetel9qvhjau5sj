# $ ifup wifi=wifi-static

rename mac/gg:gg:gg:gg:gg:gg=wifi

iface wifi-static inet static
	address 10.238.999.999
	netmask 255.255.0.0
	wireless-essid hotspot-to-be-used
	wireless-mode Ad-Hoc
	#gateway 10.238.888.888
	# dns-* options are implemented by the resolvconf package, if installed
	dns-nameservers 9.9.9.9
	dns-search wifi.emgenxx69lwyn5ctlr4nl64ul.local
