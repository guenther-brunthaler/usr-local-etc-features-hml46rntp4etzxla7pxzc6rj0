/var/log/syslogprefix {
	prerotate
		set -e
		fifo=/var/lib/misc/logrotate-4vzntsgfwksyilhv27klxte5l
		locker=/etc/cron.daily/save_logs-4vzntsgfwksyilhv27klxte5l
		test ! -p "$fifo" || mkfifo -m 600 -- "$fifo"
		"$locker" --exec-locked sh -c "read waiting < '$fifo'" &
	endscript
	postrotate
		echo > "$fifo"
	endscript
}
